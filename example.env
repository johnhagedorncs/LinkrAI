# ========================================
# 189A Project - Master Environment Variables
# ========================================
# Copy this file to .env and fill in your actual credentials
# Command: cp example.env .env
#
# This file is used by ALL components:
# - A2A-Framework agents (referral, scheduling, messaging, host)
# - Athena API wrapper
# - Demo frontend/backend
# - Scribe agent

# ========================================
# AWS Credentials (required)
# ========================================
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# ========================================
# AWS Region (required)
# ========================================
AWS_REGION=us-east-1

# ========================================
# AWS Bedrock Configuration
# ========================================
# Model ID for all Bedrock-powered agents
BEDROCK_MODEL_ID=us.anthropic.claude-haiku-4-5-20251001-v1:0

# ========================================
# Athena Health API Configuration
# ========================================
ATHENA_CLIENT_ID=your_athena_client_id
ATHENA_CLIENT_SECRET=your_athena_client_secret
ATHENA_PRACTICE_ID=195900  # Example sandbox practice ID - replace with your own
ATHENA_BASE_URL=https://api.preview.platform.athenahealth.com  # Athena sandbox environment

# ========================================
# AWS Transcription Settings (required for scribe agent)
# ========================================
# Transcription Settings (required)
# S3 bucket where transcription results will be stored
TRANSCRIBE_OUTPUT_BUCKET=artera-transcriptions

# Medical Specialty (optional, default: PRIMARYCARE)
# Options: PRIMARYCARE, CARDIOLOGY, NEUROLOGY, ONCOLOGY, RADIOLOGY, UROLOGY, OPHTHALMOLOGY, ORTHOPEDICS
TRANSCRIBE_SPECIALTY=PRIMARYCARE

# Language (optional, default: en-US)
TRANSCRIBE_LANGUAGE_CODE=en-US

# Speaker Diarization (optional, default: 2)
# Maximum number of speakers to identify (usually doctor + patient = 2)
TRANSCRIBE_MAX_SPEAKERS=2


# ========================================
# AWS End User Messaging (optional - for SMS via Messaging Agent)
# ========================================
# AWS End User Messaging Configuration (for SMS)
# Note: AWS End User Messaging is the successor to AWS Pinpoint
# Required for production SMS sending/receiving
AWS_SMS_ORIGINATION_NUMBER=+1234567890  # Your purchased phone number from AWS
AWS_SMS_CONFIGURATION_SET=  # Optional: for tracking delivery events

# Optional: Force mock mode for development (true/false)
# If AWS_SMS_ORIGINATION_NUMBER is not set, mock mode is automatic
USE_MOCK_SMS=false